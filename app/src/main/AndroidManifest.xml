<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.EnglishBeginner">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logoapp"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@drawable/logoapp"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication"
        android:usesCleartextTraffic="true">
        <activity
            android:name="com.example.EnglishBeginner.Admin.WordManagement.IetlsManagement"
            android:exported="false" />
        <activity
            android:name="com.example.EnglishBeginner.Admin.WordManagement.ToeicManagement"
            android:exported="false" />
        <activity
            android:name="com.example.EnglishBeginner.Admin.LearnManagement.LearnQuestion"
            android:exported="false" />
        <activity
            android:name="com.example.EnglishBeginner.Admin.ChangePasswordActivity_Admin"
            android:exported="false" />
        <activity
            android:name="com.example.EnglishBeginner.Admin.LearnManagement.QuestionInterface"
            android:exported="false" />
        <activity
            android:name="com.example.EnglishBeginner.Admin.LearnManagement.TopicManagement"
            android:exported="false" />
        <activity
            android:name="com.example.EnglishBeginner.Admin.LearnManagement.TypeQuestionManagement"
            android:exported="false" />
        <activity
            android:name="com.example.EnglishBeginner.Admin.LearnManagement.LevelManagement"
            android:exported="false" />
        <activity
            android:name="com.example.EnglishBeginner.Admin.AdminInterface"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.EnglishBeginner.Login.ForgetPassword"
            android:exported="false" />
        <activity
            android:name="com.example.EnglishBeginner.User.UserInterface"
            android:exported="true" />
        <activity
            android:name="com.example.EnglishBeginner.Login.RegisterUser"
            android:exported="true" />
        <activity
            android:name="com.example.EnglishBeginner.Login.Login"
            android:exported="false" />

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />

        <activity
            android:name="com.facebook.FacebookActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/app_name" />
        <activity
            android:name="com.facebook.CustomTabActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="@string/fb_login_protocol_scheme" />
            </intent-filter>
        </activity>
    </application>

</manifest>